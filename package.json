{
  "name": "swanky-theme",
  "description": "Default swanky theme",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Rod Leviton",
  "license": "MIT",
  "dependencies": {
    "basscss": "^8.0.1",
    "cz-conventional-changelog": "1.1.6",
    "cz-customizable": "3.1.0",
    "exact-semver": "1.2.0",
    "ghooks": "1.2.1",
    "semantic-release": "4.3.5",
    "validate-commit-msg": "2.6.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "exact-semver",
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swanky-docs/swanky-theme.git"
  }
}
