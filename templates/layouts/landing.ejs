<%- include('../partials/header.ejs') -%>
<main class="<%-styles['container-fluid']-%> swanky-docs" ng-app="app">
  <div class="<%-styles.row-%>">
    <!-- Sidebar -->
    <aside class="<%-styles['col-lg-2']-%> <%-styles['col-md-3']-%> <%-styles['col-sm-3']-%> <%-styles.sidebar-%>">
      <ul class="<%-styles.sidenav-%> <%-styles['dropable']-%> <%-styles['sticky']-%>">
        <% nav.forEach((item) => { %>
          <li class="<%= (item.key === key || item.key === parentKey) ? styles.active : '' %>">
          <a href="/<%-item.path-%>" title="<%-item.title-%>"><%-item.title-%></a>
          <% if (item.children) { %>
          <ul>
            <% item.children.forEach((child) => { %>
            <li class="<%= (child.key === key) ? styles.active : '' %>">
              <a href="/<%-child.path-%>" title="<%-child.title-%>">
                <%-child.title-%>
              </a>
            </li>
            <% }); %>
          </ul>
          <% } %>
        </li>
        <% }); %>
      </ul>
    </aside>
    <!-- END Sidebar -->
    
    <!-- Main content -->
    <article class="<%-styles['col-lg-10']-%> <%-styles['col-md-9']-%> <%-styles['col-sm-9']-%> <%-styles['main-content']-%>" role="main">

      <h1 class="<%-styles['page-header']-%>">
        <%- title -%>
      </h1>
      <% contents.forEach((item) => { %>
        <%- item -%>
      <% }); %>
    </article>
    <!-- END Main content -->
  </div>
</main>
<%- include('../partials/footer.ejs') -%>